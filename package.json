{
  "name": "compiler-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{js,ts,svelte,css,html}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,svelte,css,html}\"",
    "dead-code": "knip",
    "dead-code:fix": "knip --fix",
    "cleanup": "npm run lint:fix && npm run format",
    "validate": "npm run check && npm run lint && npm run dead-code"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@sveltejs/adapter-static": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@types/lz-string": "^1.3.34",
    "eslint": "^9.0.0",
    "eslint-plugin-svelte": "^3.0.0",
    "globals": "^17.0.0",
    "knip": "^5.0.0",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0",
    "vite": "^6.0.0"
  },
  "dependencies": {
    "@codemirror/commands": "^6.0.0",
    "@codemirror/lang-python": "^6.1.0",
    "@codemirror/language": "^6.0.0",
    "@codemirror/search": "^6.0.0",
    "@codemirror/state": "^6.0.0",
    "@codemirror/theme-one-dark": "^6.1.0",
    "@codemirror/view": "^6.0.0",
    "codemirror": "^6.0.0",
    "lz-string": "^1.5.0"
  },
  "type": "module"
}
